# ==============================================
# ShareUpTime Mobile Environment Variables
# ==============================================

# ==============================================
# Backend API Configuration
# ==============================================

# Backend API Base URL
API_URL=http://localhost:3000
API_TIMEOUT=10000

# WebSocket Configuration
WS_URL=ws://localhost:3000

# ==============================================
# Firebase Configuration
# ==============================================

# Firebase Core
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-app.firebaseapp.com
FIREBASE_PROJECT_ID=your-app-id
FIREBASE_STORAGE_BUCKET=your-app.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-id
FIREBASE_APP_ID=your-firebase-app-id
FIREBASE_MEASUREMENT_ID=your-measurement-id

# Firebase Dynamic Links
FIREBASE_DYNAMIC_LINK_DOMAIN=shareuptime.page.link

# ==============================================
# Push Notifications
# ==============================================

# Firebase Cloud Messaging
FCM_VAPID_KEY=your-fcm-vapid-key

# OneSignal (alternative)
ONESIGNAL_APP_ID=your-onesignal-app-id

# ==============================================
# Social Authentication
# ==============================================

# Google OAuth
GOOGLE_WEB_CLIENT_ID=your-google-web-client-id
GOOGLE_IOS_CLIENT_ID=your-google-ios-client-id
GOOGLE_ANDROID_CLIENT_ID=your-google-android-client-id

# Facebook OAuth
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_NAME=ShareUpTime
FACEBOOK_DISPLAY_NAME=ShareUpTime

# Apple Sign In
APPLE_SERVICE_ID=com.shareuptime.signin

# ==============================================
# Media & Storage
# ==============================================

# Image Configuration
MAX_IMAGE_SIZE=5242880
ALLOWED_IMAGE_TYPES=jpg,jpeg,png,gif
IMAGE_QUALITY=0.8

# Video Configuration
MAX_VIDEO_SIZE=52428800
ALLOWED_VIDEO_TYPES=mp4,mov,avi
VIDEO_QUALITY=medium

# Cache Configuration
CACHE_SIZE_LIMIT=104857600
CACHE_TTL=86400000

# ==============================================
# Maps & Location
# ==============================================

# Google Maps
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Mapbox (alternative)
MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Location Settings
LOCATION_ACCURACY=high
LOCATION_TIMEOUT=15000

# ==============================================
# Analytics & Monitoring
# ==============================================

# Google Analytics
GA_TRACKING_ID=UA-XXXXXXXX-X

# Firebase Analytics
FIREBASE_ANALYTICS_ENABLED=true

# Crashlytics
CRASHLYTICS_ENABLED=true

# ==============================================
# Development Settings
# ==============================================

# Debug Mode
DEBUG_MODE=true
LOG_LEVEL=debug

# Development Tools
FLIPPER_ENABLED=true
REACTOTRON_ENABLED=true

# Mock Data
USE_MOCK_DATA=false

# ==============================================
# App Configuration
# ==============================================

# App Information
APP_NAME=ShareUpTime
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# Deep Linking
URL_SCHEME=shareuptime
DEEP_LINK_PREFIX=shareuptime://

# ==============================================
# Security
# ==============================================

# Certificate Pinning
CERTIFICATE_PINNING_ENABLED=false
PINNED_CERTIFICATES=cert1.crt,cert2.crt

# Biometric Authentication
BIOMETRIC_ENABLED=true

# ==============================================
# Feature Flags
# ==============================================

# Features
ENABLE_DARK_MODE=true
ENABLE_OFFLINE_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_LOCATION_SHARING=true
ENABLE_VIDEO_CALLS=false

# Experimental Features
ENABLE_AR_FILTERS=false
ENABLE_LIVE_STREAMING=false

# ==============================================
# Performance
# ==============================================

# Image Caching
IMAGE_CACHE_ENABLED=true
IMAGE_CACHE_SIZE=50

# Bundle Splitting
CODE_SPLITTING_ENABLED=true

# Performance Monitoring
PERFORMANCE_MONITORING_ENABLED=true

# ==============================================
# Localization
# ==============================================

# Default Language
DEFAULT_LANGUAGE=tr
FALLBACK_LANGUAGE=en

# Supported Languages
SUPPORTED_LANGUAGES=tr,en,de,fr,es

# ==============================================
# Store Configuration
# ==============================================

# App Store
IOS_APP_ID=123456789
IOS_TEAM_ID=XXXXXXXXXX

# Google Play
ANDROID_PACKAGE_NAME=com.shareuptime.app
ANDROID_SIGNING_KEY_ALIAS=shareuptime

# ==============================================
# Environment Specific
# ==============================================

# Environment
ENVIRONMENT=development

# Build Configuration
BUILD_TYPE=debug

# Code Push (for OTA updates)
CODEPUSH_DEPLOYMENT_KEY_ANDROID=your-android-deployment-key
CODEPUSH_DEPLOYMENT_KEY_IOS=your-ios-deployment-key

# ==============================================
# Third Party Services
# ==============================================

# Sentry Error Tracking
SENTRY_DSN=your-sentry-dsn-for-mobile

# Amplitude Analytics
AMPLITUDE_API_KEY=your-amplitude-api-key

# Branch Deep Linking
BRANCH_KEY=your-branch-key

# ==============================================
# Production Overrides
# ==============================================
# Uncomment and modify for production builds

# API_URL=https://api.shareuptime.com
# WS_URL=wss://api.shareuptime.com
# DEBUG_MODE=false
# LOG_LEVEL=error
# ENVIRONMENT=production
# BUILD_TYPE=release